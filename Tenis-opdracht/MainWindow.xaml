<Window x:Class="Tenis_opdracht.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tenis_opdracht"
        Loaded="Window_Loaded"
        mc:Ignorable="d"
        Title="Tenis club AP" Height="450" Width="800">
    <Grid>
        <Grid Margin="0,0,0,0" Width="150" HorizontalAlignment="Left">
            <Button Content="Members" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" Width="55" Click="Button_Click"/>
            <Button Content="Games" HorizontalAlignment="Center" Margin="0,40,0,0" VerticalAlignment="Top" Width="55" Click="Button_Click"/>
            <Button Content="Results" HorizontalAlignment="Center" Margin="0,70,0,0" VerticalAlignment="Top" Width="55" Click="Button_Click"/>
        </Grid>
        <Frame NavigationUIVisibility="Hidden" Margin="150,0,0,0" Name="test"/>

    </Grid>
</Window>
